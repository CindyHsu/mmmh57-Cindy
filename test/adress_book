CREATE TABLE `proj57`.`address_book` (
     `sid` INT NOT NULL , 
     `name` VARCHAR(255) NOT NULL , 
     `email` VARCHAR(255) NOT NULL , 
     `mobile` VARCHAR(255) NOT NULL , 
     `birthday` DATE NOT NULL , 
     `address` VARCHAR(255) NOT NULL , PRIMARY KEY (`sid`)) ENGINE = InnoDB CHARSET=armscii8 COLLATE armscii8_general_ci;